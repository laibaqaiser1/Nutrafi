{
  "name": "nutrify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "postinstall": "prisma generate && node scripts/ensure-prisma-binaries.js",
    "build": "prisma generate && node scripts/ensure-prisma-binaries.js && next build",
    "postbuild": "node -e \"const fs=require('fs');const path=require('path');const src=path.join(__dirname,'lib/generated/prisma');const dest=path.join(__dirname,'.next/standalone/lib/generated/prisma');if(fs.existsSync('.next/standalone')&&fs.existsSync(src)){fs.mkdirSync(dest,{recursive:true});['libquery_engine-rhel-openssl-3.0.x.so.node','libquery_engine-debian-openssl-3.0.x.so.node'].forEach(f=>{const s=path.join(src,f);const d=path.join(dest,f);if(fs.existsSync(s))fs.copyFileSync(s,d);});}\"",
    "start": "next start",
    "lint": "eslint",
    "db:seed": "tsx scripts/seed-admin.ts",
    "db:import-menu": "tsx scripts/import-menu.ts",
    "db:import-meal-items": "tsx scripts/import-meal-items.ts",
    "db:seed-plans": "tsx scripts/seed-plans.ts",
    "db:import-customers": "tsx scripts/import-customers.ts",
    "db:import-meal-plans": "tsx scripts/import-meal-plans.ts",
    "db:clear-meal-plans": "tsx scripts/clear-meal-plans.ts",
    "db:cleanup-placeholders": "tsx scripts/cleanup-placeholder-customers.ts",
    "db:cleanup-duplicate-meal-plans": "tsx scripts/cleanup-duplicate-meal-plans.ts",
    "db:cleanup-customers": "tsx scripts/cleanup-customers.ts",
    "db:add-dua-meals": "tsx scripts/add-dua-meal-items.ts",
    "db:add-garry-meals": "tsx scripts/add-garry-meal-items.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.19.2",
    "adm-zip": "^0.5.16",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.3.1",
    "jspdf": "^4.1.0",
    "jspdf-autotable": "^5.0.7",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "prisma": "^6.19.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "tailwind-merge": "^3.4.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/adm-zip": "^0.5.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/xml2js": "^0.4.14",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "glob": "^13.0.3",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
